<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.cyh.mapper.EmployeeMapper">

    <select id="findByGender" resultType="com.cyh.bean.Employee">
        SELECT *
        FROM tbl_employee
        WHERE gender = #{gender}
    </select>

</mapper>